{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^8.1",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-redis": "*",
    "composer/package-versions-deprecated": "1.11.99.4",
    "doctrine/doctrine-bundle": "2.5.*",
    "doctrine/doctrine-migrations-bundle": "3.2.*",
    "doctrine/orm": "2.11.*",
    "gusapi/gusapi": "^3.3",
    "guzzlehttp/guzzle": "^7.8",
    "knplabs/knp-paginator-bundle": "^6.2",
    "lexik/jwt-authentication-bundle": "*",
    "predis/predis": "^2.2",
    "prugala/symfony-request-dto": "^1.3",
    "serwersms/serwersms-php-client": "*",
    "smsapi/php-client": "^3.0",
    "stof/doctrine-extensions-bundle": "^1.7",
    "symfony-bundles/json-request-bundle": "^4.1",
    "symfony/amqp-messenger": "6.2.*",
    "symfony/cache": "6.2.*",
    "symfony/console": "6.2.*",
    "symfony/doctrine-messenger": "6.2.*",
    "symfony/dotenv": "6.2.*",
    "symfony/flex": "^1.3.1",
    "symfony/form": "6.2.*",
    "symfony/framework-bundle": "6.2.*",
    "symfony/intl": "6.2.*",
    "symfony/lock": "6.2.*",
    "symfony/mailer": "6.2.*",
    "symfony/messenger": "6.2.*",
    "symfony/monolog-bundle": "^3.8",
    "symfony/runtime": "6.2.*",
    "symfony/security-bundle": "6.2.*",
    "symfony/twig-bundle": "6.2.*",
    "symfony/uid": "6.2.*",
    "symfony/validator": "6.2.*",
    "symfony/yaml": "6.2.*",
    "symfonycasts/verify-email-bundle": "^1.13",
    "twig/extra-bundle": "^3.5"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.35",
    "phpunit/phpunit": "^9.5",
    "symfony/browser-kit": "6.2.*",
    "symfony/css-selector": "6.2.*",
    "symfony/maker-bundle": "1.43.*",
    "symfony/phpunit-bridge": "^6.3",
    "symfony/stopwatch": "6.2.*",
    "symfony/web-profiler-bundle": "6.2.*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "App\\Application\\": "src/Application/",
      "App\\Domain\\": "src/Domain/",
      "App\\Infrastructure\\": "src/Infrastructure/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "php-cs-fixer": "vendor/bin/php-cs-fixer fix src --rules=@Symfony,@PSR12",
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "6.2.*"
    }
  }
}
